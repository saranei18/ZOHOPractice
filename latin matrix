#include<stdio.h>
void main()
{
    int a=97;
    int b=a;
    int c=a+3;
    int h=0;
    for(int i=0;i<3;i++)
    {
        b=b+i;
        for(int j=b;j<c;j++)
        {
            printf("%c ",j);
            h++;
        }
        c=a+3;
        if(h!=3)
        {
            for(int f=97;f<=99-h;f++)
            {
                printf("%c ",f);
            }
        }
        b=a;
        h=0;
        printf("\n");
        
    }
}
