#include<stdio.h>
void main()
{
    int a[]={5,7,8,1,9};
    int lis[]={1,1,1,1,1};
    for(int i=1;i<5;i++)
    {
        for(int j=0;j<i;j++)
        {
            if(a[i]>a[j]&&lis[i]<=lis[j])
            {
                lis[i]=1+lis[j];
            }
        }
    }
    int k=lis[4]+1;
    for(int i=4;i>=0;i--)
    {
        if((k-1)==lis[i])
        {
            printf("%d",a[i]);
            
        }
        k=lis[i];
    }
}
