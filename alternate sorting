#include<stdio.h>
void main()
{
    int a[]={1,2,3,4,5,6,7};
    int j=a[6];
    int k=1;
    for(int i=0;i<7;i++)
    {
        if(i%2!=0)
        {
            a[i]=k;
            k++;
        }
        else
        {
            a[i]=j;
            j--;
        }
    }
    for(int i=0;i<7;i++)
    {
        printf("%d ",a[i]);
    }
}
