#include<stdio.h>
void main()
{
    int a[]={4,6,3,7};
    for(int i=0;i<4;i++)
    {
        for(int j=i+1;j<4;j++)
        {
            for(int k=j+1;k<4;k++)
            {
                if(a[i]+a[j]>a[k]&&a[i]+a[k]>a[j]&&a[k]+a[j]>a[i])printf("%d %d %d   ",a[i],a[j],a[k]);
            }
        }
    }
}
